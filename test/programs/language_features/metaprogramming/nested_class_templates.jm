#?#2\n

pragma def Foo(T)
  class foo
    value:Bar(T)

    new(v:Bar(T))
      value = v
    end

    def print()
      value.print()
    end
  end

  return foo
end

pragma def Bar(T)
  class bar
    value:T

    new(v:T)
      value = v
    end

    def print()
      puts(value)
    end
  end

  return bar
end

b = (pragma Bar(Int))(2)
f = (pragma Foo(Int))(b)
f.print()
